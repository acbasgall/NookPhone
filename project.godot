; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="NookPhone"
run/main_scene="res://scenes/nook_phone.tscn"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

ConfigLoader="*res://scripts/config_loader.gd"
OllamaInterface="*res://scripts/ollama_interface.gd"
BrainManager="*res://scripts/brain_manager.gd"
ResidentManager="*res://scripts/resident_manager.gd"

[display]

window/size/viewport_width=450
window/size/viewport_height=800
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
